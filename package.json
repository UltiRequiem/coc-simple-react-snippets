{
  "name": "coc-simple-react-snippets",
  "displayName": "Simple React Snippets",
  "description": "Dead simple React snippets you will actually use.",
  "version": "1.3.0",
  "author": "Eliaz Bobadilla <eliaz.bobadilladev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/UltiRequiem/coc-simple-react-snippets"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/ultirequiem"
  },
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./src/javascript.json"
      },
      {
        "language": "javascript",
        "path": "./src/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/typescript.json"
      }
    ]
  }
}
